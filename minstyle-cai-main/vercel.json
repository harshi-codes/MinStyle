{
  "version": 2,
  "builds": [
    {
      "src": "minstyle-cai-main/vite.config.ts",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "cleanDistDir": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ]
}l