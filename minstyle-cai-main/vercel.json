{
  "version": 2,
  "builds": [
    {
      "src": "minstyle-cai-main/vite.config.ts",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "minstyle-cai-main/dist",
        "cleanDistDir": true
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/minstyle-cai-main/dist/index.html"
    }
  ]
}